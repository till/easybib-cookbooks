[<%= @name %>]
name=<%= @description %>
baseurl=<%= @base_url %>
repo_gpgcheck=<%= @repo_gpgcheck %>
<% if @priority -%>
priority=<%=@priority %>
<% end -%>
gpgcheck=0
enabled=1
gpgkey=<%= @gpg_url %>
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
<% if @metadata_expire %>
metadata_expire=<%= @metadata_expire %>
<% end %>
