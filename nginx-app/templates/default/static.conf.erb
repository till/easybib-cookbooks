server {
    listen 80;

    index <%=@default_router%> index.php;

    access_log <%=@access_log%>;

<% if @nginx_extra && !@nginx_extra.empty? -%>
    <%=@nginx_extra%>
<% end -%>

<% if !@domain_name.empty? -%>
    server_name <%=@domain_name%>;
<% end -%>

    location ~* \.(eot|ttf|woff)$ {
        add_header Access-Control-Allow-Origin *;
    }

    root <%= @doc_root%>;
}
